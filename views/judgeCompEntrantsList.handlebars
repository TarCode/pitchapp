<div class="section no-pad-bot" id="index-banner">

</div>
<!--CONTENT START-->
<div class="row">
    <!--COMP SECTION START-->
    <div id="comp" class="col s12">
      <a href =  "/judge/comp/{{competition_id}}/totals" class="btn-large waves-effect waves-light green">results</a>
      <div class = "container">
        <!--ENTRANTS START -->
        {{#entrants}}
        <div class="row">
          <div class="col s3"><img class = "responsive-img circle" src = "{{image_url}}"></div>
          <div class="col s6">
            <span class="flow-text">{{name}}</span>
            <br>
            <span class="">
              <p>Name: {{name}}
                    <br>
                 Location: {{location}}
                    <br>
                 Industry: {{industry}}
                    <br>
                 Sector: {{sector}}
                    <br>
                 Employees: {{employees}}
                    <br>
                 Stage: {{stage}}
                    <br>
                 Turnover: {{turnover}}
              </p>
            </span>
          </div>
          <div class = "col s3">

            <a href="/judge/{{competition_id}}/{{id}}"  class="judgeBtn hidden btn-large waves-effect waves-light blue listButton">Judge</a>

          </div>
        </div>
        <hr/>
        {{/entrants}}
          <!--ENTRANTS END -->
          <br><br>

          <div class="section">

          </div>
        </div>
    </div>
    <!--COMP SECTION END-->
</div>
